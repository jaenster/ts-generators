{
  "name": "@jaenster/generators",
  "version": "0.0.2",
  "description": "simple wrapper around generators",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "yarn && tsc && mocha -r ts-node/register test/**/*.test.ts"
  },
  "files": [
    "/bin"
  ],
  "keywords": [
    "generator",
    "generators",
    "typescript",
    "async",
    "await"
  ],
  "author": "Jaenster",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "jest": "^27.0.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^4.8.2"
  },
  "repository": "git:https://github.com/jaenster/ts-generators",
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@jaenster/events": "^0.9.6",
    "@jaenster/queues": "^0.1.4",
    "@jaenster/synchronized": "^0.1.1"
  }
}
